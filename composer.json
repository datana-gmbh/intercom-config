{
    "name": "datana-gmbh/intercom-config",
    "type": "library",
    "description": "Be able to share a config object which is common throughout all intercom instances",
    "license": "MIT",
    "authors": [
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "datana-gmbh/intercom-value-objects": "^1.2.0",
        "thecodingmachine/safe": "^1.3 || ^2.0",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "ergebnis/test-util": "^1.4",
        "phpunit/phpunit": "^9.0",
        "symfony/string": "^5.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Datana\\Intercom\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datana\\Intercom\\Config\\Tests\\": "tests/"
        }
    }
}
